/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
  IN a, b;
  OUT out;

  PARTS:
    // Put your code here:
    Nand(a=a, b=b, out=o1);
    Or(a=a, b=b, out=o2);
    And(a=o1, b=o2, out=out);
}
